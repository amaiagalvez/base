{
    "name": "amaia/base",
    "description": "Base package",
    "license": "MIT",
    "authors": [
        {
            "name": "Amaia Galvez",
            "email": "amaia@amaia.eus"
        }
    ],
    "version": "1.0.0",
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5",
        "orchestra/canvas": "^7.1"
    },
    "autoload": {
        "psr-4": {
          "Amaia\\Base\\": "src",
          "Amaia\\Base\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Amaia\\Base\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amaia\\Base\\BaseServiceProvider"
            ],
            "aliases": {
                "Calculator": "Amaia\\Base\\Facades\\Calculator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
      }
}
